<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head id="Head1" runat="server">
    <title>HCFMRP - PEP</title>
    <asp:ContentPlaceHolder ID="head" runat="server">
    </asp:ContentPlaceHolder>
    <style type="text/css">
        .white_content
        {
            position: fixed;
            top: 50%;
            left: 50%;
            width: 65px;
            height: 38px;
            margin-top: -25px;
            margin-left: -22.5px;
            padding: 10px;
            background-color: #fff;
            z-index: 1002;
            overflow: hidden;
            text-align: center;
            border: 1px solid #923222;
            -moz-border-radius: 4px;
            -webkit-border-radius: 4px;
            border-radius: 4px;
            -moz-box-shadow: 1px 1px 1px #ccc;
            -webkit-box-shadow: 1px 1px 1px #ccc;
            box-shadow: 1px 1px 1px #ccc;
            color: #923222;
            font-size: 10px;
            font-weight: bold;
        }
        .txt-loading
        {
            font-size: 10px;
            color: #923222;
        }
        .ui-widget-overlay {
            background: transparent url("../../InterfaceHC/imagens/bg_transp.png") !important;
        }
        .back_overlay
        {
            position: fixed !important;;
            top: 0% !important;;
            left: 0% !important;;
            width: 100% !important;;
            height: 100% !important;;
            background: transparent url("../../InterfaceHC/imagens/bg_transp.png") !important;
            z-index: 1001 !important;;
        }
        
        .notification
        {
            margin-bottom: 10px;
            position: fixed;
            top: 0;
            left: 0;
            z-index: 999;
            text-align: center;
            font-weight: bold;
            width: 99.9%;
        }
        .notification .messages
        {
            margin: 1px;
            padding: 13px 0px 13px 50px;
            color: rgb(43, 43, 43);
            font-size: 14px;
            text-align: center;
        }
        .erro
        {
            border: 1px solid rgb(197, 143, 117);
        }
        .erro .messages
        {
            background: url("../../InterfaceHC/imagens/error.png") no-repeat 20px 9px rgb(242, 187, 160);
        }
        .sucesso
        {
            border: 1px solid rgb(154, 201, 51);
        }
        .sucesso .messages
        {
            background: url("../../InterfaceHC/imagens/success.png") no-repeat 20px 9px rgb(206, 231, 144);
        }
        .alerta
        {
            border: 1px solid rgb(230, 210, 96);
        }
        .alerta .messages
        {
            background: url("../../InterfaceHC/imagens/warning.png") no-repeat 20px 9px rgb(249, 237, 170);
        }
        .informacao
        {
            border: 1px solid rgb(172, 219, 239);
        }
        .informacao .messages
        {
            background: url("../../InterfaceHC/imagens/info.png") no-repeat 20px 9px rgb(218, 239, 251);
        }
        
        #topo
        {
            margin: auto;
            width: 100%;
            margin-bottom: 10px;
            background: url('../../InterfaceHC/imagens/cabecalho.jpg') repeat-x top;
        }
        .fundo-faixa
        {
            margin: auto;
            width: 730px;
            height: 77px;
        }
        
        /* Paginador */
        .PagerCurrentPageCell, .PagerOtherPageCells
        {
            background-color: #f7f7f7;
        }
        .PagerCurrentPageCell
        {
            padding: 5px;
            background-color: #f4f4f4;
        }
        .PagerOtherPageCells a
        {
            padding: 5px;
        }
        .PagerOtherPageCells a:hover
        {
            padding: 5px;
            background-color: #333;
            color: #FFF;
            text-decoration: none;
        }
        
        .Grid_linha:hover
        {
            background-color: #ffe7a2;
            border-bottom: 1px solid #e8ecf0;
            color: #000000;
            cursor: hand;
            font-size: 8pt;
            padding: 1 3 1 5;
        }
        
        .legendaForm
        {
            background-color: #EDEFF1;
            color: #349802;
            font-weight: bold;
            padding-top: 10px;
        }
    </style>
    
</head>
<body style="padding-top:0px">
    <form id="form1" runat="server" class="form-horizontal" role="form">
    
    <asp:ScriptManager runat="server" EnableScriptGlobalization="true" AsyncPostBackTimeout="3600"
        ID="smMo">
    </asp:ScriptManager>
    
    <asp:ContentPlaceHolder runat="server" ID="cphCabecalho">
        
    </asp:ContentPlaceHolder>

    <asp:ValidationSummary runat="server" ID="ValidationSummaryModal" ValidationGroup="validacao"
        CssClass="warning" Width="90%" />
