            </div>
            <div style="vertical-align: middle; float: left; height: 20px;">
                Aguarde...</div>
        </ProgressTemplate>
    </asp:UpdateProgress>--%>
    

    <asp:UpdatePanel UpdateMode="Conditional" runat="server" ID="updMensagem">
        <ContentTemplate>
            <asp:Panel Style="display: none; border: none; padding: 0px; margin: 0px" runat="server"
                ID="pnlMensagem">
                <div class="messages">
                    <asp:Label runat="server" ID="lblMensagem"></asp:Label>
                </div>
            </asp:Panel>
        </ContentTemplate>
    </asp:UpdatePanel>

    <asp:ContentPlaceHolder ID="ContentPlaceHolder1" runat="server">
        
